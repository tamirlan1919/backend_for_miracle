{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "orders"
    },
    "total_price": {
      "type": "decimal"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "оплачен",
        "не оплачен",
        "в пути",
        "в обработке",
        "отменен"
      ]
    }
  }
}
